{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "Azure": "Warning",
      "Consumer": "Debug"
    },
    "Console": {
      "ActivityTrackingOptions": "SpanId,TraceId,ParentId,Baggage,Tags",
      "FormatterName": "simple",
      "FormatterOptions": {
        "IncludeScopes": true,
        "SingleLine": true,
        "TimestampFormat": "HH:mm:ss "
      }
    }
  }
}
